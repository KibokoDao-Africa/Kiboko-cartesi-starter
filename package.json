{
  "name": "kiboko-cartesi",
  "version": "0.4.41",
  "description": "setup of a Cartesi DApp monorepo, making it easier and quicker for developers to start building innovative blockchain solutions.",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/DennohKim/Kiboko-cartesi-starter",
  "bugs": "https://github.com/DennohKim/Kiboko-cartesi-starter/issues",
  "repository": "https://github.com/DennohKim/Kiboko-cartesi-starter",
  "license": "MIT",
  "author": "Dennis Kimathi",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "kiboko-cartesi": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "git add README.md"
  },
  "oclif": {
    "bin": "kiboko-cartesi",
    "commands": "./dist/commands",
    "dirname": "kiboko-cartesi",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.16",
    "simple-git": "^3.23.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^20",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.6.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}